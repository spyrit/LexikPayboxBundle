{
    "name": "lexik/paybox-bundle",
    "type": "symfony-bundle",
    "description": "LexikPayboxBundle eases the implementation of the Paybox payment system.",
    "keywords": ["Symfony2", "bundle", "Paybox", "Payment"],
    "homepage": "https://github.com/lexik/LexikPayboxBundle",
    "license": "MIT",
    "authors": [
        { "name": "Lexik", "email": "dev@lexik.fr" },
        { "name": "Olivier Maisonneuve", "email": "o.maisonneuve@lexik.fr" }
    ],
    "require": {
        "php": ">=8.0.2",
        "psr/log": "~1.0|~2.0",
        "lib-curl": ">=7.10.0",
        "lib-openssl": ">=0.9.6",
        "symfony/framework-bundle": "~5.0",
        "symfony/form": "~5.0",
        "symfony/options-resolver": "~5.0",
        "kriswallsmith/buzz": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": { "Lexik\\Bundle\\PayboxBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x.x-dev"
        }
    }
}
